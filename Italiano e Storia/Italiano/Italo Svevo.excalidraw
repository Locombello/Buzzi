    {
      "id": "MgouCBkPFdOR4VVdYorYD",
      "type": "text",
      "x": 1625.777777777778,
      "y": 463.30555555555577,
      "width": 870.3236083984375,
      "height": 560,
      "angle": 0,
      "strokeColor": "#000000",
      "backgroundColor": "transparent",
      "fillStyle": "hachure",
      "strokeWidth": 2,
