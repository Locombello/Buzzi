    {
      "id": "b4VEIpIEFX_JYMr_-cU8f",
      "type": "text",
      "x": 1903.4668776244027,
      "y": 82.49195300212051,
      "width": 727,
      "height": 800,
      "angle": 0,
      "strokeColor": "#000000",
      "backgroundColor": "transparent",
      "fillStyle": "hachure",
      "strokeWidth": 1,
      "strokeStyle": "solid",
